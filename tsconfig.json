{
	"compilerOptions": {
		"declaration": true,
		"stripInternal": true,
		"target": "es5",
		"lib": [
			"DOM",
			"es6"
		],
		"outDir": "dist/",
		"moduleResolution": "node",
		"isolatedModules": false,
		"noImplicitAny": false,
		"removeComments": false,
		"noLib": false,
		"sourceMap": false
	},
	"include": [
		"src"
	]
}
